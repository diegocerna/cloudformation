    "PrivateSubnetId" : {
      "Type" : "String",
      "Description" : "Subnet id that the api belongs",
      "Default" : "subnet-d511f3fe"
    },

    "gtSubnetID" : {
      "Type" : "String",
      "Default" : "subnet-2e16f405",
      "Description" : "Subnet ID for GT worker"
    },

